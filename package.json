{
  "name": "@chialab/dna",
  "version": "3.22.0",
  "description": "Progressive Web Components",
  "type": "module",
  "main": "dist/cjs/dna.cjs",
  "module": "dist/esm/dna.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "browser": "./dist/browser/dna.js",
      "node": "./dist/node/dna.js",
      "require": "./dist/cjs/dna.cjs",
      "default": "./dist/esm/dna.js"
    },
    "./jsx-runtime": "./jsx-runtime.js",
    "./jsx-dev-runtime": "./jsx-runtime.js"
  },
  "files": [
    "dist",
    "types",
    "jsx-runtime.js",
    "jsx-runtime.d.ts",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chialab/dna"
  },
  "bugs": {
    "url": "https://github.com/chialab/dna/issues"
  },
  "homepage": "https://www.chialab.io/p/dna",
  "author": "Chialab <dev@chialab.io> (https://www.chialab.it)",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && yarn types && rna build",
    "types": "rimraf types && tsc",
    "test": "yarn test:typings && yarn test:browser && yarn test:node",
    "test:typings": "tsc -p test/typings --noEmit",
    "test:browser": "rna test:browser test/*.spec.js --coverage",
    "test:saucelabs": "rna test:saucelabs test/*.spec.js --browsers 'android 6.0' 'android 13.0' 'ios 12.0' 'ios 16.0' 'safari 12' 'safari 15' 'safari 16' 'firefox 78' 'firefox latest-1' 'firefox latest' 'chrome 45' 'chrome latest-1' 'chrome latest' 'edge 17.17134' 'edge latest'",
    "test:node": "rna test:node test/*.spec.js",
    "lint": "publint && prettier --check . && eslint src test",
    "docs": "node scripts/apidoc.js",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@changesets/cli": "^2.22.0",
    "@chialab/eslint-config": "^4.0.0",
    "@chialab/ginsenghino": "^1.0.0",
    "@chialab/prettier-config": "^1.2.2",
    "@chialab/rna": "^0.17.0",
    "@chialab/rna-browser-test-runner": "^0.17.0",
    "@chialab/rna-bundler": "^0.17.0",
    "@chialab/rna-node-test-runner": "^0.17.0",
    "@chialab/rna-saucelabs-test-runner": "^0.17.0",
    "core-js": "^3.6.4",
    "eslint": "^8.0.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "htm": "^3.0.3",
    "mocha": "^8.4.0",
    "prettier": "^3.0.3",
    "publint": "^0.2.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@chialab/quantum": "^1.0.0-alpha.0",
    "@types/jsdom": "^21.1.3",
    "jsdom": "^22.1.0"
  }
}
