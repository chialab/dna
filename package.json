{
  "name": "@chialab/dna",
  "version": "3.21.6",
  "description": "Progressive Web Components",
  "type": "module",
  "main": "dist/cjs/dna.js",
  "module": "dist/esm/dna.js",
  "types": "types/index.d.ts",
  "browser": {
    "jsdom": false
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/dna.cjs",
      "node": "./dist/node/dna.js",
      "default": "./dist/esm/dna.js"
    },
    "./jsx-runtime": "./jsx-runtime.js",
    "./analyzer": "./analyzer/index.js"
  },
  "files": [
    "dist",
    "types",
    "analyzer",
    "jsx-runtime.js",
    "jsx-runtime.d.ts",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chialab/dna"
  },
  "bugs": {
    "url": "https://github.com/chialab/dna/issues"
  },
  "homepage": "https://www.chialab.io/p/dna",
  "author": "Chialab <dev@chialab.io> (https://www.chialab.it)",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && yarn types && rna build",
    "types": "rimraf types && tsc --project tsconfig.build.json",
    "test": "yarn test:typings && yarn test:browser && yarn test:node",
    "test:typings": "tsc -p test/typings --noEmit",
    "test:browser": "rna test:browser test/*.spec.js --coverage",
    "test:saucelabs": "rna test:saucelabs test/*.spec.js --browsers 'modern' 'legacy'",
    "test:node": "rna test:node test/*.spec.js",
    "test:integrations": "rna test:browser test/integrations/*.spec.js",
    "lint": "eslint src analyzer test",
    "docs": "(echo \"# API Reference\n\"; rna apidoc src/index.ts -F markdown) > docs/API.md",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "jsdom": "^17.0.0"
  },
  "peerDependenciesMeta": {
    "jsdom": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/jsdom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@changesets/cli": "^2.22.0",
    "@chialab/eslint-config": "^3.0.0",
    "@chialab/ginsenghino": "^1.0.0",
    "@chialab/rna": "^0.17.0",
    "@chialab/rna-apidoc": "^0.17.0",
    "@chialab/rna-browser-test-runner": "^0.17.0",
    "@chialab/rna-bundler": "^0.17.0",
    "@chialab/rna-node-test-runner": "^0.17.0",
    "@chialab/rna-saucelabs-test-runner": "^0.17.0",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@types/jsdom": "^16.0.0",
    "core-js": "^3.6.4",
    "eslint": "^8.0.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "htm": "^3.0.3",
    "jsdom": "^17.0.0",
    "lit": "^2.2.7",
    "mocha": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "typescript": "^4.4.0",
    "vue": "^3.2.37"
  }
}
