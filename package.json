{
  "name": "@chialab/dna",
  "version": "4.4.0-beta.9",
  "description": "Progressive Web Components",
  "type": "module",
  "main": "dist/dna.js",
  "module": "dist/dna.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/dna.js"
    },
    "./jsx-runtime": {
      "types": "./types/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./types/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    }
  },
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chialab/dna.git"
  },
  "bugs": {
    "url": "https://github.com/chialab/dna/issues"
  },
  "homepage": "https://chialab.github.io/dna/",
  "author": "Chialab <dev@chialab.io> (https://www.chialab.it)",
  "scripts": {
    "build": "rimraf 'dist' 'types' && yarn types && esbuild dna=src/index.ts jsx-runtime=src/jsx-runtime.ts --splitting --outdir=dist --out-extension:.js=.js --format=esm --bundle --packages=external --sourcemap",
    "types": "tsc",
    "test": "yarn test:typings && yarn test:browser && yarn test:node",
    "test:typings": "tsc -p test --noEmit",
    "test:browser": "vitest run --browser",
    "test:browserstack": "vitest run --config vitest.browserstack.ts",
    "test:node": "vitest run",
    "lint": "biome check",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "htm": "^3.0.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.22.0",
    "@chialab/vitest-provider-browserstack": "^0.19.0-beta.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/vue": "^8.1.0",
    "@types/react": "^19.1.6",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.0.0",
    "@vitest/coverage-istanbul": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "axe-core": "^4.10.3",
    "esbuild": "^0.25.0",
    "ivya": "^1.6.0",
    "lit": "^3.3.0",
    "playwright": "^1.50.0",
    "preact": "^10.26.6",
    "publint": "^0.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.0",
    "svelte": "^5.33.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.0",
    "uhtml": "^4.7.1",
    "vite": "^7.0.0",
    "vitepress": "^1.0.0",
    "vitest": "^3.0.0",
    "vue": "^3.5.14"
  },
  "resolutions": {
    "esbuild": "^0.25.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
